<tables>
<table>
<name>vtiger_cbswebinaraccess</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbswebinaraccess` (
  `cbswebinaraccessid` int(11) DEFAULT NULL,
  `webinaraccessno` varchar(100) DEFAULT NULL,
  `access_date` date DEFAULT NULL,
  `access_time` time DEFAULT NULL,
  `relid` integer(11) DEFAULT NULL,
  `access_url` varchar(100) DEFAULT NULL,
  `srvprovidor` varchar(100) DEFAULT NULL,
  `attended` varchar(3) DEFAULT NULL,
  `websiteid` varchar(100) DEFAULT NULL,
  `extcmpid` varchar(100) DEFAULT NULL,
  `cmpid` int(11) DEFAULT NULL,
  PRIMARY KEY (`cbswebinaraccessid`),
  index(`relid`),
  index(`cmpid`),
  index(`websiteid`),
  index(`extcmpid`),
  index(`access_date`,`access_time`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
<table>
<name>vtiger_cbswebinaraccesscf</name>
<sql><![CDATA[CREATE TABLE `vtiger_cbswebinaraccesscf` (
  `cbswebinaraccessid` int(11) NOT NULL,
  PRIMARY KEY (`cbswebinaraccessid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
